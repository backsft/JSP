  <%
  	Connection con=null;
  	Statement stmt=null;
  
  
  
  try {
		Class.forName("com.mysql.jdbc.Driver"); 
		 
		 con=DriverManager.getConnection(  
		"jdbc:mysql://localhost:3306/intelji","root","admin");  			
		 stmt=con.createStatement(); 
			String a= "delete from student where rollNo='12222'";
			stmt.executeUpdate(a);	
		 	System.out.print("data sucsssfully deleted from the database");

		 	stmt.close();
      		con.close();
  }
  catch (Exception e)
  {
      e.printStackTrace();
  
  }
  
  
  
  %>
    
    
   
    
   
 
